<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="Fore" Color="White" />
    <SolidColorBrush x:Key="WindowBack" Color="#2D2D30" />
    <SolidColorBrush x:Key="ToolBack" Color="#252526" />
    <SolidColorBrush x:Key="ToolbarBtnBack" Color="Transparent" />
    <SolidColorBrush x:Key="TreeSelected" Color="LightGoldenrodYellow" />
    <SolidColorBrush x:Key="StatusBarBack" Color="#007ACC" />
    <SolidColorBrush x:Key="PropertyNameFore" Color="IndianRed" />
    <SolidColorBrush x:Key="PropertyValueFore" Color="CadetBlue" />

</ResourceDictionary>